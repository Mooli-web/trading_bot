{
  "rl_params": {
    "gamma": {
      "type": "float",
      "range": [0.95, 0.999]
    },
    "gae_lambda": {
      "type": "float",
      "range": [0.9, 0.99]
    },
    "clip_range": {
      "type": "float",
      "range": [0.1, 0.3]
    },
    "ent_coef": {
      "type": "float",
      "range": [1e-8, 0.01],
      "log": true
    },
    "learning_rate": {
      "type": "float",
      "range": [1e-5, 1e-3],
      "log": true
    },
    "n_steps": {
      "type": "choice",
      "values": [1024, 2048, 4096]
    },
    "stop_loss_atr_multiplier": {
      "type": "float",
      "range": [1.5, 5.0]
    },
    "take_profit_atr_multiplier": {
      "type": "float",
      "range": [1.0, 6.0]
    }
  }
}